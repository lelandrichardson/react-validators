  "bugs": {
    "url": "https://github.com/lelandrichardson/react-validators/issues"
  },
  "homepage": "https://github.com/lelandrichardson/react-validators#readme",
  "devDependencies": {
    "babel": "^5.8.23",
    "chai": "^3.4.0",
    "mocha": "^2.3.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "prop-types": "^15.5.0"
